        </binding>
      </customBinding>
    </bindings>
    <client>
      <endpoint address="http://3g.worldsms.cn/sdkwebservice/sdkwebservice.asmx"
        binding="basicHttpBinding" bindingConfiguration="commonService"
        contract="HuaXiaSms.commonService" name="commonService" />
      <endpoint address="http://3g.worldsms.cn/sdkwebservice/sdkwebservice.asmx"
